{
  "name": "local-classification-semantic",
  "version": "1.0.0",
  "description": "Derive semantics from heterogenous open data",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "build/src"
  ],
  "license": "Apache-2.0",
  "keywords": [],
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest test/test.js --runInBand --forceExit",
    "check": "gts check src/**/*.ts src/*.ts",
    "clean": "gts clean src/**/*.ts src/*.ts",
    "compile": "tsc",
    "build": "npm run fix && npm run check && npm run compile",
    "fix": "gts fix src/**/*.ts src/*.ts",
    "exec": "tsc && node build/index",
    "docs": "swagger-jsdoc -d docs/swagger.definition.json -o docs/swagger.json src/index.ts && java -jar ../swagger-codegen-cli.jar generate -i ./docs/swagger.json -l html2 -o ./docs/"
  },
  "devDependencies": {
    "@types/node": "^14.17.3",
    "@types/pg": "^8.6.0",
    "eslint": "^7.28.0",
    "gts": "^3.1.0",
    "jest": "^27.0.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@google-cloud/translate": "^6.2.2",
    "cologne-phonetic": "^1.0.1",
    "dotenv": "^10.0.0",
    "fastest-levenshtein": "^1.0.12",
    "local-logger": "file:../local-logger",
    "local-microservice": "file:../local-microservice",
    "metaphone": "^2.0.0",
    "node-snowball": "^0.6.0",
    "pg": "^8.6.0",
    "stopwords-de": "^0.3.0"
  }
}
