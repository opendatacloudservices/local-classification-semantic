{
  "name": "@opendatacloudservices/local-classification-semantic",
  "version": "1.0.0",
  "description": "Derive semantics from heterogenous open data",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "engines": {
    "npm": ">=8.4.1",
    "node": ">=17.5.0"
  },
  "files": [
    "build"
  ],
  "license": "Apache-2.0",
  "keywords": [],
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendatacloudservices/local-classification-semantic.git"
  },
  "author": "sebastian-meier",
  "bugs": {
    "url": "https://github.com/opendatacloudservices/local-classification-semantic/issues"
  },
  "homepage": "https://github.com/opendatacloudservices/local-classification-semantic#readme",
  "scripts": {
    "test": "jest test/test.js --runInBand --forceExit",
    "check": "gts check src/**/*.ts src/*.ts",
    "clean": "gts clean src/**/*.ts src/*.ts",
    "compile": "tsc",
    "build": "npm run fix && npm run check && npm run compile",
    "fix": "gts fix src/**/*.ts src/*.ts",
    "exec": "tsc && node build/index",
    "docs": "swagger-jsdoc -d docs/swagger.definition.json -o docs/swagger.json src/index.ts && java -jar ../swagger-codegen-cli.jar generate -i ./docs/swagger.json -l html2 -o ./docs/"
  },
  "dependencies": {
    "@google-cloud/translate": "^6.3.1",
    "@opendatacloudservices/local-logger": "^1.0.3",
    "@opendatacloudservices/local-microservice": "^1.2.0",
    "cologne-phonetic": "^1.0.1",
    "dotenv": "^16.0.0",
    "fastest-levenshtein": "^1.0.12",
    "metaphone": "^2.0.0",
    "node-snowball": "^0.6.0",
    "pg": "^8.7.3",
    "stopwords-de": "^0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.20",
    "@types/pg": "^8.6.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "eslint": "^7.32.0",
    "gts": "^3.1.0",
    "jest": "^27.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.4.4"
  }
}
